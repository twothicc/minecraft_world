{
    "format_version": "1.10.0",
    "animation_controllers": {
		"controller.animation.mrun": {
            "states": {
                "default": {
                    "transitions": [{
                            "dead": "variable.health_percent = (query.health / query.max_health) * 20.0; return variable.health_percent > 10.0;"
                        }
                    ],
                    "on_entry": [
						"@s mt"
                    ]
                },
                "dead": {
                    "transitions": [{
                            "default": "variable.health_percent = (query.health / query.max_health) * 20.0; return variable.health_percent > 10.0;"
                        }
                    ],
                    "on_entry": [
                        "@s run"
                    ]
                }
            }
        }
    }
}